page {
    background-color: #ffffff;
}

.integralView {
    height: 180rpx;
    background-color: #deedf0;
    display: flex;
    justify-content: center;
    align-items: center;
}

.integralView view {
    height: 80rpx;
    background-color: #80bac6;
    font-size: 36rpx;
    line-height: 12rpx;
    color: #ffffff;
    width: 400rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 40rpx;
}
.comsume{
    height: 88rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 26rpx;
}
.comsume>view:nth-of-type(2){
    flex: 1;
}
.comsume>view:nth-of-type(2){
    font-size: 32rpx;
    letter-spacing: 2rpx;
    line-height: 40rpx;
    flex: 5;
    margin-left: 26rpx;
}
.comsume>view:nth-of-type(2)>view:nth-of-type(2){
    font-size: 26rpx;
    color: #a0a0a0;
}
.comsume>view:nth-of-type(3){
    flex: 1;
    color:#58a4b4 ;
    font-size: 36fpx;
letter-spacing: 2fpx;
line-height: 40rpx;
}