.submit {
    width: 100%;
    background-color: #58a4b4;
    font-size: 36rpx;
    line-height: 36rpx;
    color: #ffffff;
    height: 100rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    position: absolute;
    bottom: 0;
}

.address{
    height: 124rpx;
    background-color: #ffffff;
    display: flex;
    padding: 17rpx 0rpx;
    align-items: center;
    border-bottom: 1rpx solid #cccccc;
}
.address>view:nth-of-type(1),.address>view:nth-of-type(2),.address>view:nth-of-type(3){
display: flex;
justify-content: center;
align-items: center;
}
.address>view:nth-of-type(1){
flex: 2;
}
.address>view:nth-of-type(2){
flex: 7;
flex-wrap: wrap;
flex-direction: column;
justify-content: center;
align-items: flex-start;
font-size: 32rpx;
line-height: 65rpx;
color: #333333;
/* height: 73rpx; */
margin-bottom: 30rpx;
}
.address>view:nth-of-type(2)>view:nth-of-type(2){
    font-size: 26rpx;
    line-height: 12rpx;
    color: #666666;
}
.address>view:nth-of-type(3){
flex: 1;
flex-direction: column;
justify-content: space-around;
height: 100%;
}