page {
    background-color: #ffffff;
}

swiper,
swiper-item {
    width: 100vw;
    height: 500rpx;
}

swiper+view {
    height: 125rpx;
    border-bottom: 15rpx #f5f5f5 solid;
    font-size: 30rpx;
    line-height: 30rpx;
    color: #333333;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0rpx 30rpx;
}

.van-cell {
    height: 100rpx;
}

.van-cell__title {
    font-size: 30rpx;
    line-height: 30rpx;
    color: #333333;
    display: flex;
    align-items: center;
}

swiper+view text:last-child {

    color: #58a4b4;
}


.delimiter {
    height: 84rpx;
    background-color: #f5f5f5;
    display: flex;
    align-items: center;
    justify-content: center;

}

.delimiter>view:nth-of-type(2) {
    margin: 0px 15rpx;
    font-size: 28fpx;
    line-height: 34rpx;
    color: #cccccc;
}

.delimiter>view:nth-of-type(2n+1) {
    width: 90rpx;
    height: 2rpx;
    background-color: #cccccc;
}

.count,
.commentsTags {
    height: 20rpx;
    padding: 30rpx;
    font-size: 30rpx;
    letter-spacing: 0rpx;
    line-height: 30rpx;
    color: #333333;
}

.commentsTags {
    padding-top: 0;
    border-bottom: 1rpx solid #cccccc;
    height: 50rpx;
}

.commentsTags view {
    font-size: 26rpx;
    line-height: 12rpx;
    color: #999999;
    width: 160rpx;
    height: 50rpx;
    border-radius: 25px;
    border: 1rpx solid #cccccc;
    display: flex;
    justify-content: center;
    align-items: center;
}

.commentsBody {
    margin: 30rpx;
    display: flex;
    align-items: flex-start;
}

.commentsBody>view {
    margin-left: 30rpx;
}

.commentsBody>view>view:nth-of-type(1) {
    font-size: 30rpx;
    line-height: 34rpx;
    color: #333333;
    display: flex;
    justify-content: space-between;
}

.commentsBody>view>view:nth-of-type(1) text:nth-of-type(2) {
    font-size: 22rpx;
    line-height: 22rpx;
    color: #999999;
}

.commentsBody>view>view:nth-of-type(2) {
    padding: 20rpx 0rpx;
    border-bottom: 1rpx solid #cccccc;
    font-size: 28rpx;
    letter-spacing: 1rpx;
    line-height: 36rpx;
    color: #666666;
}

.commentsBody>view>view:nth-of-type(2)>view:nth-of-type(2) {
    margin: 30rpx 0rpx 0rpx 0rpx;
}

.body>view:last-child {
    display: flex;
    justify-content: center;
    padding: 37rpx 0rpx;
}

.body>view:last-child view {
    width: 280rpx;
    height: 70rpx;
    border-radius: 35rpx;
    background-color: #ffffff;
    border: 1px solid #58a4b4;
    display: flex;
    justify-content: center;
    align-items: center;
}

.business {
    padding: 20rpx 0rpx;
    font-size: 28rpx;
    letter-spacing: 1rpx;
    line-height: 36rpx;
    color: #666666;
}

.information,
.information+view {
    font-size: 30rpx;
    line-height: 34rpx;
    color: #333333;
}

.information,
.information+view>view {
    padding: 30rpx;
}

.information+view>view {
    display: flex;
    align-items: center;
}

.information+view>view>text:last-child {
    font-size: 28rpx;
    line-height: 30rpx;
    color: #999999;
}

.submit {
    width: 100%;
    background-color: #58a4b4;
    font-size: 36rpx;
    line-height: 36rpx;
    color: #ffffff;
    height: 90rpx;
    display: flex;
    position: fixed;
    bottom: 0;
}

.submit>view {
    height: 100%;
}

.submit>view:nth-of-type(1),
.submit>view:nth-of-type(2),
.submit>view:nth-of-type(3) {
    display: flex;
    justify-content: center;
    align-items: center;
}

.submit>view:nth-of-type(1) {
    background-color: #f0f0f0;
    flex: 3;
    flex-direction: column;
    font-size: 24rpx;
    line-height: 24rpx;
    color: #666666;
    justify-content: space-evenly;
}

.submit>view:nth-of-type(2),
.submit>view:nth-of-type(3) {
    background-color: #58a4b4;
    flex: 4;
    font-size: 30rpx;
    line-height: 30rpx;
    color: #ffffff;

}

.submit>view:nth-of-type(2) {
    border-right: 1rpx solid #cccccc;
}