.address>view:nth-of-type(2){
    flex: 7;
    flex-wrap: wrap;
    flex-direction: column;
    justify-content: center;
    align-items: flex-start;
    font-size: 32rpx;
    line-height: 65rpx;
    color: #333333;
    /* height: 73rpx; */
    margin-bottom: 30rpx;
    }

.address>view:nth-of-type(2)>view:nth-of-type(2) {
    font-size: 26rpx;
    line-height: 12rpx;
    color: #666666;
}

.address>view:nth-of-type(3) {
    flex: 1;
    flex-direction: column;
    justify-content: space-around;
    height: 100%;
}

.submit {
    width: 100%;
    background-color: #58a4b4;
    font-size: 30rpx;
    line-height: 36rpx;
    color: #ffffff;
    height: 100rpx;
    display: flex;
    position: fixed;
    bottom: 0;
    justify-content: flex-end;
    align-items: center;
    background-color: #ffffff;
}

.submit>view {
    width: 200rpx;
    height: 60rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 30px;
    margin-right: 30rpx;
    background-color: #58a4b4;
}


.van-cell {
    height: 100rpx;
    border-bottom: 1rpx solid #cccccc;
    display: flex;
    align-items: center;
}

.van-cell__title,
.van-cell__value {
    font-size: 30rpx;
    line-height: 32rpx;
    color: #333333;
}

.waring {
    background-color: #fff;
    font-size: 24rpx;
    line-height: 24rpx;
    color: #58a4b4;
    display: flex;
    font-size: 40rpx;
    line-height: 42rpx;
    justify-content: center;
    align-items: center;
    height: 120rpx;
    margin-bottom: 20rpx;
}


.orderInfo {
    background-color: #ffffff;
    padding: 10rpx;
    border-bottom: 1rpx solid #f5f5f5;
}

.orderInfo>view {
    padding: 0rpx 30rpx;
}

.orderInfo>view:nth-of-type(1) {
    display: flex;
    font-size: 30rpx;
    line-height: 40rpx;
    color: #333333;
    flex-wrap: wrap;
    padding: 34rpx auto;
    margin: 30rpx 0rpx;
    align-items: center;
}


.orderInfo>view:nth-of-type(1)>view {
    flex: 1;
}

.orderInfo>view:nth-of-type(1)>view>view {
    display: flex;
    justify-content: space-between;
    align-items: center;
    flex-wrap: wrap;
}

.orderInfo>view:nth-of-type(1)>view>view:nth-of-type(2) {
    color: #999999;
}

.orderInfo>view:nth-of-type(1)>view>view:nth-of-type(3) text:nth-of-type(1) {
    color: #999999;
}

.orderInfo>view:nth-of-type(1)>view {
    padding-left: 30rpx;
}