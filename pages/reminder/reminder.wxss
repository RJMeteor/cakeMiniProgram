.address {
    height: 124rpx;
    background-color: #ffffff;
    display: flex;
    padding: 17rpx 0rpx;
    align-items: center;
    border-bottom: 1rpx solid #cccccc;
    margin-bottom: 15rpx;
}

.address>view:nth-of-type(1),
.address>view:nth-of-type(2),
.address>view:nth-of-type(3) {
    display: flex;
    justify-content: center;
    align-items: center;
}

.address>view:nth-of-type(1) {
    flex: 2;
}

